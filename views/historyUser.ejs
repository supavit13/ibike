<!DOCTYPE html>
<html>

<head>
    <% include head.ejs %>
</head>
<body>
    <% include navbar.ejs %>
    <div style="margin:5%">
        <h1>HistoryUseMorcyc</h1>
        <table class="ui selectable celled table">
            <thead>
              <tr>
                <th>Date/Time</th>
                <th>MorcycID</th>
                <th>Time</th>
                <th>Cost</th>
                <th>Balance</th>
              </tr>
            </thead>
            <tbody>
                <% for(var i=0;i<(Hisuser['historyUser']).length;i++) { %>
              <tr>
                <td><%= Hisuser['historyUser'][0][i]['dateUse'] %></td>
                <td><%= Hisuser['historyUser'][0][i]['IDmorcyc'] %></td>
                <td><%= Hisuser['historyUser'][0][i]['time'] %></td>
                <td><%= Hisuser['historyUser'][0][i]['cost'] %></td>
                <td><%= Hisuser['historyUser'][0][i]['balance'] %></td>
              </tr>
                <% } %>
            </tbody>
          </table>
    </div>
</body>

</html>