<!DOCTYPE html>
<html>

<head>
    <% include head.ejs %>
</head>

<body>
    <% include navbar.ejs %>
        <div id="map"></div>

        <script>
            var mymap = L.map('map', {
                center: new L.LatLng(13.1192, 100.92066),
                zoom: 16
            });
            mymap.setMaxBounds([
                [21.22794, 90.79102],
                [21.30985, 115.00488],
                [3.73271, 90.79102],
                [4.82826, 115.00488]
            ]);
            L.gridLayer.googleMutant({
                minZoom: 6,
                type: 'roadmap' // valid values are 'roadmap', 'satellite', 'terrain' and 'hybrid'
            }).addTo(mymap);
            
        </script>

        <div>
            <center>
                <a href="/price">
                    <button class="ui primary button"> Booking </button>
                </a>
            </center>
        </div>
</body>

</html>